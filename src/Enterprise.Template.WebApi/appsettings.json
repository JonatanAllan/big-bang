{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQSettings": {
    "HostName": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "UtilityConnectionString": "<<user-secrets>>"
  },
  "LoggingSettings": {
    "APIVersion": 1.0,
    "ReleaseNumber": "#{Release.ReleaseName}#",
    "ServiceID": "Enterprise.Template.WebApi",
    "LogFilePath": "logs.txt",
    "UseMessageQueue": "true"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "WriteTo": []
  }
}
